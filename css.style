body {
    font-family:Arial, Helvetica, sans-serif;
    font-size: 14px;
    background-color: rgb(103, 102, 102);
    

}
thead {
    font-weight: bold;
    background-color: darkseagreen;
}
th {
    border-right-color: 2px, solid rgb(44, 32, 67);
}
h1 {
    font-weight: 900;
    text-align: center;
}
h2{ 
    font-weight: 600;
    text-align: center;
}
table {
    border: 2px, solid rgb(44, 32, 67);
    width: 650px;
    margin: 0 auto;
    background-color: paleturquoise;
    text-align: center;
}
td {
    border-top: 2px, solid rgb(44, 32, 67);
}

.selector {
    display: inline;
    background-color: peachpuff;
    font-family: monospace;
    
}
